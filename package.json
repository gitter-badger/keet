{
  "name": "keet",
  "version": "3.0.0",
  "description": "Minimalist view layer for the web",
  "main": "keet.js",
  "scripts": {
    "start": "webpack-dev-server -d",
    "test": "mocha test/test.js --require babel-core/register",
    "cover": "nyc --reporter=lcov --reporter=text npm run test",
    "test-ci": "nyc npm run test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "bundle": "webpack --config webpack.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syarul/keet.git"
  },
  "keywords": [
    "templating",
    "minimal",
    "view layer"
  ],
  "author": "Shahrul Nizam Selamat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/syarul/keet/issues"
  },
  "homepage": "https://github.com/syarul/keet#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "coveralls": "^3.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.5.1",
    "markdown-magic": "^0.1.20",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "pug": "^2.0.0-rc.4",
    "pug-html-loader": "^1.1.5",
    "uglify-js": "^3.3.8",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {},
  "nyc": {
    "include": [
      "keet.js",
      "components"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "istanbul"
    ]
  }
}
