{
  "name": "keet",
  "version": "0.9.0",
  "description": "A data-driven view, OO, pure js without new paradigm shift",
  "main": "index.js",
  "scripts": {
    "distcompress": "browserify index.js --standalone Keet | uglifyjs > dist/keet.min.js",
    "dist": "browserify index.js --standalone Keet > dist/keet.js",
    "jsdoc": "jsdoc2md index.js > docs/DOCUMENTATION.md",
    "test": "gulp",
    "cov": "browserify -t coverify test/v2.js | tape-run | coverify",
    "cov2": "browserify -t coverify test/v2.js | tape-run | coverify | node_modules\\.bin\\tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syarul/keet.git"
  },
  "keywords": [
    "virtual dom",
    "web component",
    "web view layer",
    "template string",
    "framework"
  ],
  "author": "Shahrul Nizam Selamat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/syarul/keet/issues"
  },
  "homepage": "https://github.com/syarul/keet#readme",
  "devDependencies": {
    "browserify": "^14.3.0",
    "coverify": "^1.4.1",
    "gulp": "^3.9.1",
    "gulp-livereload": "^3.8.1",
    "gulp-shell": "^0.5.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "tape-run": "^3.0.0"
  }
}
