{
  "name": "keet",
  "version": "2.3.0",
  "description": "Minimalist view layer for the web",
  "main": "rebase.js",
  "scripts": {
    "start": "parcel index.html",
    "debug": "browserify index.js --debug --standalone Keet > dist/keet.debug.js",
    "dist": "browserify index.js --standalone Keet > dist/keet.js && node markdown.config.js",
    "distcompress": "browserify index.js --standalone Keet | uglifyjs > dist/keet.min.js",
    "test": "mocha test/test.js --require babel-core/register",
    "cover": "nyc --reporter=lcov --reporter=text npm run test",
    "test-ci": "nyc npm run test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syarul/keet.git"
  },
  "keywords": [
    "templating",
    "minimal",
    "view layer"
  ],
  "author": "Shahrul Nizam Selamat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/syarul/keet/issues"
  },
  "homepage": "https://github.com/syarul/keet#readme",
  "devDependencies": {
    "markdown-magic": "^0.1.20",
    "parcel-bundler": "^1.4.1",
    "string-to-stream": "^1.1.0"
  },
  "dependencies": {}
}
