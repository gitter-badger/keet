{
  "name": "keet",
  "version": "3.5.0",
  "author": "Shahrul Nizam Selamat",
  "description": "Minimalist view layer for the web",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "istanbul"
    ]
  },
  "bugs": {
    "url": "https://github.com/syarul/keet/issues"
  },
  "dependencies": {
    "hash-sum": "^1.0.2",
    "jsdom-global": "^3.0.2",
    "set-dom": "^7.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "browserify": "^15.2.0",
    "coveralls": "^3.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.5.1",
    "markdown-magic": "^0.1.20",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "pug": "^2.0.0-rc.4",
    "pug-html-loader": "^1.1.5",
    "uglify-js": "^3.3.8",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "homepage": "https://github.com/syarul/keet#readme",
  "keywords": [
    "templating",
    "minimal",
    "view layer"
  ],
  "license": "MIT",
  "main": "keet.js",
  "nyc": {
    "include": [
      "keet.js",
      "components"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syarul/keet.git"
  },
  "scripts": {
    "bundle": "browserify keet.js --standalone Keet | uglifyjs --compress --mangle > keet-min.js && node markdown.config.js",
    "cover": "nyc --reporter=lcov --reporter=text npm run test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "start": "webpack-dev-server -d",
    "test": "mocha test/test.js --require babel-core/register --require jsdom-global/register",
    "test-ci": "nyc npm run test"
  },
  "git_branch": "master"
}
